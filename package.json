{
  "devDependencies": {
    "cpx": "^1.5.0",
    "typescript": "^3.0.3",
    "wasm-worker": "^0.3.1"
  },
  "scripts": {
    "asbuild:untouched": "asc assembly/index.ts -b build/untouched.wasm -t build/untouched.wat --sourceMap --validate",
    "asbuild:optimized": "asc -O3 assembly/index.ts -b build/optimized.wasm -t build/optimized.wat --sourceMap --validate --optimize --noDebug",
    "asbuild": "npm run asbuild:untouched && npm run asbuild:optimized",
    "serve": "cpx build/optimized.wasm* dist && parcel index.html"
  }
}